<!-- INHERITANCE -->

<!-- SUMMARY -->
@if(@Model.Summary != null)
{
	<div class="alert alert-info">
		<p>@Documentation.Comment(@Model.Summary)</p>
	</div>
}

<!-- SYNTAX -->
<div class="panel panel-default">
	<div class="panel-heading"><b>Syntax</b></div>
	<div class="panel-body">
		<pre><code class="language-cs">@Documentation.Syntax(@Model.Method)</code></pre>
	</div>
</div>

<!-- EXAMPLE -->
@if(@Model.Example != null)
{	
	<div class="panel panel-default">
		<div class="panel-heading"><b>Example</b></div>
		<div class="panel-body">
			<pre>@Documentation.Comment(@Model.Example)</pre>
		</div>
	</div>
}